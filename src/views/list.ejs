

    <h1 class="text-center text-2xl text-cinza font-semibold mb-5 ">MINHAS CARTELAS</h1>
    <hr class="mb-10">


    <div class="relative   sm:rounded-lg max-h-[550px]  border ">
        
        <table class="min-w-full divide-y divide-gray-200">
            <thead class="text-xs text-gray-50 uppercase bg-cinza ">
                <tr>
                    <th scope="col" class="px-6 py-3">
                        Titulo
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Categoria
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Bingos qtd.
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Cartela qtd.
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Padr√£o
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Editar
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Trancar
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Gerar PDF
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Abrir
                    </th>
                    <th scope="col" class="px-6 py-3">
                        Deletar
                    </th>
                </tr>
            </thead>
            <tbody class="border border-red-600">
    
                <% cards.map(element => { %>
                    <tr class="odd:bg-white   even:bg-gray-50  border-b hover:bg-green-100">
                        <td title="<%= element.title %>" class="px-6 text-left py-4 font-medium text-gray-900 whitespace-nowrap   border border-gray-200 overflow-hidden ">
                            <%= element.title %>
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden text-left">
                            <%= element.category %>
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden text-center">
                            <%= element.bingosForCards %>
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden text-center">
                            <%= element.NumberOfCards %>
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden text-center">
                            <%= element.bingoPattern %>
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden">

                            <% if (typeof element.trancado != "undefined" && element.trancado && element.trancado == true ) { %>
                                <!-- trancado -->
                                <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-file-lock" viewBox="0 0 16 16">
                                    <path d="M8 5a1 1 0 0 1 1 1v1H7V6a1 1 0 0 1 1-1m2 2.076V6a2 2 0 1 0-4 0v1.076c-.54.166-1 .597-1 1.224v2.4c0 .816.781 1.3 1.5 1.3h3c.719 0 1.5-.484 1.5-1.3V8.3c0-.627-.46-1.058-1-1.224M6.105 8.125A.64.64 0 0 1 6.5 8h3a.64.64 0 0 1 .395.125c.085.068.105.133.105.175v2.4c0 .042-.02.107-.105.175A.64.64 0 0 1 9.5 11h-3a.64.64 0 0 1-.395-.125C6.02 10.807 6 10.742 6 10.7V8.3c0-.042.02-.107.105-.175"/>
                                    <path d="M4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zm0 1h8a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1"/>
                                </svg>
                            <% } else { %>
                                <!-- destrancado -->
                                <a href="/editar-cartela?id=<%= element._id %>" class="font-medium text-blue-600  hover:underline m-auto block">
                                    <img src="/icons/icons8-editar-100.png" class="w-6" alt="">
                                </a>
                            <% } %>
                            
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden block">
                           
                            <div class=" m-auto flex items-center justify-center  w-full" >
                               

                                <% if (typeof element.trancado != "undefined" && element.trancado && element.trancado == true ) { %>
                                    <!-- trancado -->
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-file-lock" viewBox="0 0 16 16">
                                        <path d="M8 5a1 1 0 0 1 1 1v1H7V6a1 1 0 0 1 1-1m2 2.076V6a2 2 0 1 0-4 0v1.076c-.54.166-1 .597-1 1.224v2.4c0 .816.781 1.3 1.5 1.3h3c.719 0 1.5-.484 1.5-1.3V8.3c0-.627-.46-1.058-1-1.224M6.105 8.125A.64.64 0 0 1 6.5 8h3a.64.64 0 0 1 .395.125c.085.068.105.133.105.175v2.4c0 .042-.02.107-.105.175A.64.64 0 0 1 9.5 11h-3a.64.64 0 0 1-.395-.125C6.02 10.807 6 10.742 6 10.7V8.3c0-.042.02-.107.105-.175"/>
                                        <path d="M4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zm0 1h8a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1"/>
                                    </svg>
                                <% } else { %>
                                    <!-- destrancado -->
                                    <button onclick="trancar('<%= element._id %>')">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-unlock-fill" viewBox="0 0 16 16">
                                            <path d="M11 1a2 2 0 0 0-2 2v4a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V9a2 2 0 0 1 2-2h5V3a3 3 0 0 1 6 0v4a.5.5 0 0 1-1 0V3a2 2 0 0 0-2-2"/>
                                          </svg>
                                    </button>
                                <% } %>
                                     
                            </div>
                            
                        </td>
                        <td class="px-6 py-4  border-gray-200 overflow-hidden border border-red ">

                            <% if (typeof element.trancado != "undefined" && element.trancado && element.trancado == true) { %>
                                <!-- trancado -->
                                <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-file-lock" viewBox="0 0 16 16">
                                    <path d="M8 5a1 1 0 0 1 1 1v1H7V6a1 1 0 0 1 1-1m2 2.076V6a2 2 0 1 0-4 0v1.076c-.54.166-1 .597-1 1.224v2.4c0 .816.781 1.3 1.5 1.3h3c.719 0 1.5-.484 1.5-1.3V8.3c0-.627-.46-1.058-1-1.224M6.105 8.125A.64.64 0 0 1 6.5 8h3a.64.64 0 0 1 .395.125c.085.068.105.133.105.175v2.4c0 .042-.02.107-.105.175A.64.64 0 0 1 9.5 11h-3a.64.64 0 0 1-.395-.125C6.02 10.807 6 10.742 6 10.7V8.3c0-.042.02-.107.105-.175"/>
                                    <path d="M4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zm0 1h8a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1"/>
                                </svg>
                            <% } else { %>
                                <!-- destrancado -->
                                <button onclick="buildPDF('<%= element._id %>')" target="_blank"  class="font-medium text-blue-600  hover:underline m-auto block">
                                    <img src="/icons/icons8-pdf-100.png" class="w-6" alt="">
                                </button>
                            <% } %>

                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden ">
                            <% if (typeof element.title != "undefined" && element.title) { %>                                     
                                <a href="/pdf/<%= element.title  %>.pdf" target="_blank"  class="font-medium text-blue-600  hover:underline m-auto block">
                                    Abrir
                                </a>
                            <% } %>
            
                        </td>
                        <td class="px-6 py-4 border border-gray-200 overflow-hidden ">
                            <button onclick="deleteCard('<%= element._id %>')" class="font-medium text-blue-600  hover:underline m-auto block">
                                <img src="/icons/icons8-remover-100.png" class="w-6" alt="">
                            </button>
                        </td>
                    </tr>
                <% }) %>
    
            </tbody>
        </table>
    
        
    </div>

    


<script src="/js/page/card.js"></script>
<script src="/js/page/cardsList.js"></script>